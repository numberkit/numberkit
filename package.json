{
  "type": "module",
  "name": "numberkit",
  "description": "A collection of mathematical utilities for JavaScript",
  "version": "0.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rimraf dist && rollup -c",
    "build:docs": "rimraf docs && typedoc",
    "format": "rome format . --write",
    "format:check": "rome format .",
    "lint": "rome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.28.2",
    "esbuild": "^0.17.4",
    "rimraf": "^4.1.2",
    "rollup": "^3.11.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rome": "^11.0.0",
    "typedoc": "^0.23.24",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4",
    "vitest": "^0.28.2"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "Kevin Hermawan",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numberkit/numberkit-js.git"
  },
  "bugs": {
    "url": "https://github.com/numberkit/numberkit-js/issues"
  },
  "homepage": "https://github.com/numberkit/numberkit-js#readme",
  "keywords": [
    "math",
    "mathematics",
    "utilities",
    "functions",
    "operations",
    "numerical",
    "computation",
    "arithmetic"
  ]
}
